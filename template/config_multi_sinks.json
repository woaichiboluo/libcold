{
    "logs": {
        "name": "main-logger",
        "level": "info",
        "flush-level": "error",
        "pattern": "%T %L <%N:%t> %c [%b:%l]%n",
        "sinks": [
            {
                "type": "basicfilesink",
                "arg1": "cur.log",
                "arg2": true
            },
            {
                "type": "stdoutcolorsink",
                "pattern": "%T %C%L%E <%N:%t> %c [%b:%l]%n"
            }
        ]
    },
    "http": {
        "timeoutms": 1000,
        "max-url-size": 190000,
        "max-header-field-size": 1024,
        "max-header-value-size": 10240,
        "max-headers-count": 100,
        "max-body-size": 1048576
    }
}